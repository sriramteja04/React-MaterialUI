{
    "name": "aws-react",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@material-ui/core": "^4.4.3",
        "@material-ui/icons": "^4.4.3",
        "amazon-cognito-identity-js": "^3.1.3",
        "amazon-cognito-js": "^1.1.0",
        "aws-amplify": "^1.1.40",
        "aws-sdk": "^2.555.0",
        "d3": "^5.12.0",
        "immutable": "^4.0.0-rc.12",
        "node-sass": "^4.12.0",
        "react": "^16.9.0",
        "react-dom": "^16.9.0",
        "react-redux": "^7.1.1",
        "react-router-dom": "^5.1.0",
        "react-scripts": "3.1.2",
        "redux": "^4.0.4",
        "redux-saga": "^1.1.1"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "compile:scss": "node-sass src/scss/index.scss src/scss/index.comp.scss",
        "prefix:scss": "postcss --use autoprefixer -b 'last 10 versions' src/scss/index.comp.scss -o src/scss/index.prefix.scss",
        "compress:scss": "node-sass src/scss/index.prefix.scss src/scss/index.comp.scss --output-style compressed",
        "build:scss": "npm-run-all compile:scss prefix:scss compress:scss"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx,scss,html,json}": [
            "pretty-quick --staged",
            "git add"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "autoprefixer": "^9.7.1",
        "gulp": "^4.0.2",
        "gulp-autoprefixer": "^7.0.1",
        "gulp-clean-css": "^4.2.0",
        "gulp-postcss": "^8.0.0",
        "gulp-sass": "^4.0.2",
        "gulp-sourcemaps": "^2.6.5",
        "husky": "^3.0.9",
        "lint-staged": "^9.4.2",
        "npm-run-all": "^4.1.5",
        "postcss-cli": "^6.1.3",
        "prettier": "^1.18.2",
        "pretty-quick": "^2.0.0"
    }
}
